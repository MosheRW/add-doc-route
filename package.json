{
  "name": "@mosherw/add-doc-route",
  "summary": "add-doc-route, a simple way to add OpenAPI documentation to your express app",
  "version": "2.0.2",
  "description": "add-doc-route, a simple way to add OpenAPI documentation to your express app, you simply need to add path to YAML file of your OpenAPI spec and you're good to go",
  "tags": [
    "express",
    "openapi",
    "swagger",
    "redocly",
    "documentation",
    "api",
    "typescript",
    "nodejs",
    "cli",
    "cli-tool"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "type": "commonjs",
  "private": false,
  "publisher": "mosherw",
  "homepage": "https://github.com/MosheRW/add-doc-route",
  "bugs": "https://github.com/MosheRW/add-doc-route/issues",
  "maintainers": [
    "mosherw"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MosheRW/add-doc-route.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org/ --access public",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com/ --access public",
    "publish:all": "npm run build && npm run publish:npm && npm run publish:github"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^13.1.0",
    "js-yaml": "^4.1.0",
    "open": "^10.1.2",
    "redoc-express": "^2.1.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "typescript": "^5.8.3"
  }
}
